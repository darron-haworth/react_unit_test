{
  "name": "react_unit_test",
  "version": "1.0.0",
  "description": "React Unit test Start-up",
  "main": "index.js",
  "scripts": {
    "test": "mocha -w test/helpers/browser.js test/*.spec.js",
    "dev:hot": "webpack-dev-server --hot --inline --progress --colors --watch --content-base ./",
    "jest": "jest"
  },
  "ava": {
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test))\\.(js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "author": "Joy Patel <joy.patel@outlook.com>",
  "license": "ISC",
  "dependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "enzyme": "^2.8.0",
    "jsdom": "^9.12.0",
    "mathjs": "^3.10.1",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "sinon": "^2.1.0",
    "webpack": "2.1.0-beta.21",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "axios": "^0.15.3",
    "babel-jest": "^19.0.0",
    "jest": "^19.0.2",
    "md5": "^2.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "regenerator-runtime": "^0.10.3",
    "ts-jest": "^19.0.6"
  }
}
